# AWS Configuration
AWS_REGION=us-east-1

# Temp use
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_SESSION_TOKEN=your_session_token_here  # Only needed for temporary credentials

# Alternatively, use an Amazon Bedrock API key (takes precedence when set)
# AWS_BEARER_TOKEN_BEDROCK=your_bedrock_api_key_here

# Or fetch the API key from AWS Secrets Manager
# BEDROCK_API_KEY_SECRET_NAME=agent-mesh/bedrock/key
# AWS_SECRETS_MANAGER_REGION=us-east-1
# BEDROCK_API_KEY_SECRET_PROFILE=your_aws_cli_profile  # optional

# Optional Configuration Overrides
# BEDROCK_MODEL_ID=amazon.nova-pro-v1:0
# FAISS_INDEX_PATH=data/policy_index.faiss
# MAX_AGENT_ROUNDS=3
# LOG_LEVEL=INFO
